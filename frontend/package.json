{
    "name": "umfragetool-ut",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --port 3000",
        "prettify": "prettier -w src/",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview --port 5050",
        "test:unit": "vitest --environment jsdom",
        "test": "c8 vitest --run",
        "test:e2e": "TS_NODE_PROJECT='tests/tsconfig.json' codeceptjs run",
        "test:e2e:windows": "(set TS_NODE_PROJECT=tests/tsconfig.json) && codeceptjs run",
        "test:e2e:def": "TS_NODE_PROJECT='tests/tsconfig.json' codeceptjs def && mv steps.d.ts tests/typings/",
        "test:e2e:def:windows": "(set TS_NODE_PROJECT=tests/tsconfig.json) && codeceptjs def && mv steps.d.ts tests/typings/",
        "report": "c8 report --reporter html",
        "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@quasar/extras": "^1.15.10",
        "date-fns": "^2.29.3",
        "ky": "^0.33.2",
        "lodash": "^4.17.21",
        "pinia": "^2.0.29",
        "quasar": "^2.11.5",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@codeceptjs/configure": "^0.10.0",
        "@quasar/vite-plugin": "^1.3.0",
        "@rushstack/eslint-patch": "^1.2.0",
        "@types/jsdom": "^20.0.1",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.11.18",
        "@types/plotly.js-dist-min": "^2.3.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/test-utils": "^2.2.7",
        "@vue/tsconfig": "^0.1.3",
        "c8": "^7.12.0",
        "codeceptjs": "^3.3.7",
        "eslint": "^8.32.0",
        "eslint-plugin-vue": "^9.9.0",
        "jsdom": "^21.1.0",
        "playwright": "^1.30.0",
        "prettier": "^2.8.3",
        "ts-node": "^10.9.1",
        "typescript": "~4.9.4",
        "vite": "^4.0.4",
        "vitest": "^0.28.2",
        "vue-tsc": "^1.0.24"
    }
}
